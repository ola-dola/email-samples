{
    "name": "email-samples",
    "version": "1.0.0",
    "description": "Custom email templates",
    "main": "index.js",
    "scripts": {
        "start": "serve ./output",
        "approval": "mjml ./mjml/account-approval.mjml -o ./output/account-approval.html",
        "activation": "mjml ./mjml/activation-code.mjml -o ./output/activation-code.html",
        "online": "mjml ./mjml/back-online.mjml -o ./output/back-online.html",
        "forgot": "mjml ./mjml/forgot-password.mjml -o ./output/forgot-password.html",
        "goodbye": "mjml ./mjml/goodbye.mjml -o ./output/goodbye.html",
        "newsletter": "mjml ./mjml/newsletter.mjml -o ./output/newsletter.html",
        "verify": "mjml ./mjml/verify-email.mjml -o ./output/verify-email.html",
        "welcome": "mjml ./mjml/welcome.mjml -o ./output/welcome.html"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ola-dola/email-samples.git"
    },
    "keywords": [
        "email samples",
        "mjml"
    ],
    "author": "Ola Oredola",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/ola-dola/email-samples/issues"
    },
    "homepage": "https://github.com/ola-dola/email-samples#readme",
    "devDependencies": {
        "serve": "^14.0.1"
    },
    "dependencies": {
        "mjml": "^4.13.0"
    }
}